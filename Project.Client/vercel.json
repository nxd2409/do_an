{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/project.cli/browser",
  "env": {
    "NG_APP_API_URL": "@ng_app_api_url",
    "NG_APP_SIGNALR_URL": "@ng_app_signalr_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
